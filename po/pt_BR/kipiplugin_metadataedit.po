#: exif/exiflight.cpp:94
msgid "Yes, compulsory, return light"
msgstr "Sim, obrigatório, com luz de retorno"

#: exif/exiflight.cpp:95
msgid "No, compulsory"
msgstr "Não, obrigatório"

#: exif/exiflight.cpp:96
msgid "No, auto"
msgstr "Não, automaticamente"

#: exif/exiflight.cpp:97
msgid "Yes, auto"
msgstr "Sim, automaticamente"

#: exif/exiflight.cpp:98
msgid "Yes, auto, no return light"
msgstr "Sim, automaticamente, sem luz de retorno"

#: exif/exiflight.cpp:99
msgid "Yes, auto, return light"
msgstr "Sim, automaticamente, com luz de retorno"

#: exif/exiflight.cpp:100
msgid "No flash function"
msgstr "Sem função de flash"

#: exif/exiflight.cpp:101
msgid "Yes, red-eye"
msgstr "Sim, olho vermelho"

#: exif/exiflight.cpp:102
msgid "Yes, red-eye, no return light"
msgstr "Sim, olho vermelho, sem luz de retorno"

#: exif/exiflight.cpp:103
msgid "Yes, red-eye, return light"
msgstr "Sim, olho vermelho, com luz de retorno"

#: exif/exiflight.cpp:104
msgid "Yes, compulsory, red-eye"
msgstr "Sim, obrigatoriamente, olho vermelho"

#: exif/exiflight.cpp:105
msgid "Yes, compulsory, red-eye, no return light"
msgstr "Sim, obrigatoriamente, olho vermelho, sem luz de retorno"

#: exif/exiflight.cpp:106
msgid "Yes, compulsory, red-eye, return light"
msgstr "Sim, obrigatoriamente, olho vermelho, com luz de retorno"

