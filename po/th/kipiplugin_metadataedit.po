msgctxt "exposure mode"
msgid "Auto"
msgstr "อัตโนมัติ (Auto)"

#: exif/exifdevice.cpp:200
msgctxt "exposure mode"
msgid "Manual"
msgstr "แมนนวล (M)"

#: exif/exifdevice.cpp:201
msgctxt "exposure mode"
msgid "Auto bracket"
msgstr "ถ่ายคร่อมค่าแสงอัตโนมัติ"

#: exif/exifdevice.cpp:202
