#, kde-format
msgid "Only Me"
msgstr "Iba ja"

#: fbalbum.cpp:56
#, kde-format
msgid "Only Friends"
msgstr "Iba priatelia"

#: fbalbum.cpp:58
#, kde-format
msgid "Friends of Friends"
