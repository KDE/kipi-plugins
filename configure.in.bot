if test -z "$ac_imlib2_config"; then
  echo ""
  echo "You're missing Imlib2. The slideshow plugin won't be compiled without Imlib2."
  echo ""
  all_tests=bad
fi

if test "x$have_libkipi" != "xyes"; then
  echo ""
  echo "You're missing libkipi. kipi-plugins will not be compiled."
  echo ""
  all_tests=bad
fi
